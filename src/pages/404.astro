---
import Mpeg from "../images/404.webp";
import Layout from "../layouts/layout.astro";
---

<Layout title="404" description="404 not found">
  <div class="hero">
    <div class="hero_1" style="display: block">
      <div class="hero-left left-string">
        <h1 class="hero-title english hobofoto">404 not found</h1>
        <div class="summary japanese">アドレスが見つかりません</div>
      </div>
      <div class="hero-right">
        <img src={Mpeg.src} alt="top" class="hero-image" />
      </div>
    </div>
  </div>
</Layout>
